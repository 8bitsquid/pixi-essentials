    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript": "^1.0.1",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "typescript": "^3.8.3"
